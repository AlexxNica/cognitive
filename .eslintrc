{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "never"
    ]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "angular": 1,
    "d3": 1,
    "$": 1,
    "_": 1
  },
  "extends": "eslint:recommended"
}
